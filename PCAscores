function [demeanpixelPCAscores_element, pixelPCAscores_element, PCAvector_element] = PCAscores(pix_time_elements, timeWindow, str)
    if strcmp(str, 'ElementWisePCA')
        [demeanpixelPCAscores_element, pixelPCAscores_element, PCAvector_element] = Ret_ElementWise(pix_time_elements);
    
    elseif strcmp(str, 'PooledPCA')
        [demeanpixelPCAscores_element, pixelPCAscores_element, PCAvector_element]  = Ret_Pool(pix_time_elements);
    
    else 
        [demeanpixelPCAscores_element, pixelPCAscores_element, PCAvector_element] = Ret_Old(pix_time_elements, timeWindow);
    
    end
end
